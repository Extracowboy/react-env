{
  "name": "react-env",
  "version": "1.0.0",
  "description": "Modern React environment with Typescript, Webpack, Babel & other useful stuff",
  "main": "index.tsx",
  "scripts": {
    "start": "run-p start:dev",
    "start:dev": "webpack serve --config webpack.config.dev.ts --port 3000",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "lint:fix": "npm run lint -- --fix",
    "test": "exit 1",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "run-p clean-dist lint",
    "build": "webpack --config webpack.config.prod.ts",
    "deploy": "exit 1"
  },
  "keywords": [],
  "author": "Alexander Evseev",
  "license": "ISC",
  "dependencies": {
    "@types/mini-css-extract-plugin": "^1.4.1",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-router-dom": "^5.1.7",
    "bootstrap": "^4.6.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.12",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.13.0",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^3.11.2",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.1.4",
    "eslint": "^7.22.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.2",
    "fork-ts-checker-webpack-plugin": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "ts-node": "^9.1.1",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
